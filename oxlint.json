{
  "$schema": "https://raw.githubusercontent.com/oxc-project/oxc/main/npm/oxlint/configuration_schema.json",
  "rules": {
    "no-unused-vars": "warn",
    "no-console": "warn",
    "eqeqeq": "error"
  },
  "ignorePatterns": [
    "node_modules",
    ".next",
    ".svelte-kit",
    "dist",
    ".turbo",
    "*.config.js",
    "*.config.mjs",
    "*.config.ts"
  ]
}
